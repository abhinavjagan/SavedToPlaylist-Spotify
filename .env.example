# Copy this to .env and fill values before running or set these env vars in your host
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=
SECRET_KEY=replace-with-random-secret
# Optional: production settings
FLASK_ENV=production

# If you deploy behind a TLS-terminating proxy (Render/Railway), set this
# to the public URL of your site, e.g. https://example.com
PUBLIC_URL=
